/* logs.wxss */
.container {
  padding: 20rpx;
}

.circle-container {
  margin-top: 20rpx;
}

h2 {
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}



.car-detail-table {
  width: 100%;
  margin-top: 20px;
  border-collapse: collapse;
}

.row {
  display: flex;
  justify-content: space-between;
}

.cell-header {
  width: 50%;
  height: 60px;
  text-align: center;
  line-height: 60px;
  border: 1px solid #000;
}

.cell {
  width: 50%;
  height: 60px;
  text-align: center;
  line-height: 60px;
  border: 1px solid #000;
}
